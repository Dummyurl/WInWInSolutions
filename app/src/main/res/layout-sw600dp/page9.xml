<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/toplayout"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="#FFFFFF" >

        <RelativeLayout
            android:id="@+id/rl_header"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="10dp" >

            <ImageView
                android:id="@+id/applogo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:background="@drawable/applogo" />

            <RelativeLayout
                android:id="@+id/rlnew"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="15dp"
                android:layout_toLeftOf="@+id/rlopen" >

                <Button
                    android:id="@+id/btn_new"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/newconflictbtn" />

                <TextView
                    android:id="@+id/txtnew"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/btn_new"
                    android:layout_marginTop="10dp"
                    android:text="New Conflict"
                    android:textColor="#000000"
                    android:textSize="18dp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rlopen"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="15dp"
                android:layout_toLeftOf="@+id/rlclose" >

                <Button
                    android:id="@+id/btn_open"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/openconflictbtn" />

                <TextView
                    android:id="@+id/txtopen"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/btn_open"
                    android:layout_marginTop="10dp"
                    android:text="Open Conflict"
                    android:textColor="#000000"
                    android:textSize="18dp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rlclose"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true" >

                <Button
                    android:id="@+id/btn_close"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/closeconflictbtn" />

                <TextView
                    android:id="@+id/txtclose"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/btn_close"
                    android:layout_marginTop="10dp"
                    android:text="Close Conflict"
                    android:textColor="#000000"
                    android:textSize="18dp" />
            </RelativeLayout>
        </RelativeLayout>

        <ScrollView
            android:id="@+id/scrollView1"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_below="@+id/rl_header"
            android:background="#FFFFFF"
            android:fadeScrollbars="false"
            android:fillViewport="true" >

            <LinearLayout
                android:id="@+id/firstscrolllayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="10dp" >

                    <TextView
                        android:id="@+id/textView1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/underline_page9"
                        android:textColor="#00478f"
                        android:textSize="24dp" />

                    <Button
                        android:id="@+id/btnreset"
                        style="?android:attr/buttonStyleSmall"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:background="@drawable/reseticonfile" />
                </RelativeLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="10dp"
                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/textView2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Title"
                        android:textColor="#00478f"
                        android:textSize="23dp" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" >

                        <EditText
                            android:id="@+id/title"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_toLeftOf="@+id/imghint"
                            android:layout_weight="1"
                            android:background="#f1f1f1"
                            android:ems="10"
                            android:gravity="top"
                            android:layout_marginRight="10dp"
                            android:hint="Provide a title for your conflict"
                            android:inputType="textMultiLine"
                           android:paddingLeft="5dp"
                           android:paddingRight="5dp"
                           android:paddingTop="7dp"
                           android:paddingBottom="7dp"
                             >
                        </EditText>

                        <ImageView
                            android:id="@+id/imghint"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_alignParentRight="true"
                            android:background="@drawable/hintt" />
                    </RelativeLayout>
                </LinearLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="40dp"
                    android:layout_marginTop="10dp" >

                    <TextView
                        android:id="@+id/textView15"
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:text="" />

                    <TextView
                        android:id="@+id/textView22"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_toRightOf="@+id/textView15"
                        android:paddingLeft="10dp"
                        android:text="Assumptions"
                        android:textColor="#00478f"
                        android:textSize="23dp" />

                    <TextView
                        android:id="@+id/textView23"
                        android:layout_width="60dp"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:text="" />

                    <TextView
                        android:id="@+id/textView24"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_toRightOf="@+id/textView23"
                        android:text="Ideas"
                        android:textColor="#00478f"
                        android:textSize="23dp" />
                </RelativeLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="50dp"
                    android:layout_marginTop="10dp" >

                    <TextView
                        android:id="@+id/textView3"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="10dp"
                        android:text="A?"
                        android:textColor="#00478f"
                        android:textSize="23dp" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" >

                        <TextView
                            android:id="@+id/txt_a"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@+id/textView4"
                            android:layout_alignParentTop="true"
                            android:layout_margin="1dp"
                            android:layout_toLeftOf="@+id/textView4"
                            android:background="#f1f1f1"
                            android:fadeScrollbars="false"
                            android:gravity="top"
                            android:hint="Why or what for do I need Objective A? Write about your assumptions why you think you need A"
                            android:maxLines="200"
                            android:padding="5dp"
                            android:scrollbars="vertical"
                            android:text=""
                            android:textColor="#000000"
                            android:textSize="23dp" />

                        <EditText
                            android:id="@+id/edt_a"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@+id/textView4"
                            android:layout_alignParentTop="true"
                            android:layout_toRightOf="@+id/textView4"
                            android:ems="10"
                            android:gravity="top"
                            android:hint="Create possible solutions (based on the assumptions and on intuition)"
                            android:inputType="textMultiLine"
                            android:padding="5dp"
                            android:textColor="#000000"
                            android:textSize="23dp" />

                        <TextView
                            android:id="@+id/textView4"
                            android:layout_width="10dp"
                            android:layout_height="100dp"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:text="" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="50dp"
                    android:layout_marginTop="2dp" >

                    <TextView
                        android:id="@+id/textView6"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="10dp"
                        android:text="AB?"
                        android:textColor="#00478f"
                        android:textSize="23dp" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" >

                        <TextView
                            android:id="@+id/txt_ab"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@+id/textView7"
                            android:layout_alignParentTop="true"
                            android:layout_marginLeft="1dp"
                            android:layout_toLeftOf="@+id/textView7"
                            android:background="#f1f1f1"
                            android:fadeScrollbars="false"
                            android:gravity="top"
                            android:hint="Assumption why is B necessary in order to get A? Complete the sentence &quot;In order to have A, I need to do B because...&quot; "
                            android:padding="5dp"
                            android:scrollbars="vertical"
                            android:text=""
                            android:textColor="#000000"
                            android:textSize="23dp" />

                        <EditText
                            android:id="@+id/edt_ab"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@+id/textView7"
                            android:layout_alignParentTop="true"
                            android:layout_toRightOf="@+id/textView7"
                            android:ems="10"
                            android:gravity="top"
                            android:hint="Create possible solutions (based on the assumptions and on intuition)"
                            android:inputType="textMultiLine"
                            android:padding="5dp"
                            android:textColor="#000000"
                            android:textSize="23dp" />

                        <TextView
                            android:id="@+id/textView7"
                            android:layout_width="10dp"
                            android:layout_height="100dp"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:text="" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="50dp"
                    android:layout_marginTop="2dp" >

                    <TextView
                        android:id="@+id/textView9"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="10dp"
                        android:text="AC?"
                        android:textColor="#00478f"
                        android:textSize="23dp" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" >

                        <TextView
                            android:id="@+id/txt_ac"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@+id/textView10"
                            android:layout_alignParentTop="true"
                            android:layout_margin="1dp"
                            android:layout_toLeftOf="@+id/textView10"
                            android:background="#f1f1f1"
                            android:fadeScrollbars="false"
                            android:gravity="top"
                            android:hint="Assumption why is C necessary in order to get A? Complete the sentence &quot;In order to have A, I need to do C because...&quot;"
                            android:padding="5dp"
                            android:scrollbars="vertical"
                            android:text=""
                            android:textColor="#000000"
                            android:textSize="23dp" />

                        <EditText
                            android:id="@+id/edt_ac"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@+id/txt_ac"
                            android:layout_alignParentTop="true"
                            android:layout_toRightOf="@+id/textView10"
                            android:ems="10"
                            android:gravity="top"
                            android:hint="Create possible solutions (based on the assumptions and on intuition)"
                            android:inputType="textMultiLine"
                            android:padding="5dp"
                            android:textColor="#000000"
                            android:textSize="23dp" />

                        <TextView
                            android:id="@+id/textView10"
                            android:layout_width="10dp"
                            android:layout_height="100dp"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:text="" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="50dp"
                    android:layout_marginTop="2dp" >

                    <TextView
                        android:id="@+id/textView12"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="10dp"
                        android:text="BD?"
                        android:textColor="#00478f"
                        android:textSize="23dp" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" >

                        <TextView
                            android:id="@+id/txt_bd1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@+id/textView13"
                            android:layout_alignParentTop="true"
                            android:layout_marginLeft="1dp"
                            android:layout_toLeftOf="@+id/textView13"
                            android:background="#f1f1f1"
                            android:fadeScrollbars="false"
                            android:gravity="top"
                            android:hint="Assumption why is D necessary in order to get B? Complete the sentence &quot;In order to have B, I need to do D because...&quot; "
                            android:padding="5dp"
                            android:scrollbars="vertical"
                            android:text=""
                            android:textColor="#000000"
                            android:textSize="23dp" />

                        <EditText
                            android:id="@+id/edt_bd1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@+id/textView13"
                            android:layout_alignParentTop="true"
                            android:layout_toRightOf="@+id/textView13"
                            android:ems="10"
                            android:gravity="top"
                            android:hint="Create possible solutions (based on the assumptions and on intuition)"
                            android:inputType="textMultiLine"
                            android:padding="5dp"
                            android:textColor="#000000"
                            android:textSize="23dp" />

                        <TextView
                            android:id="@+id/textView13"
                            android:layout_width="10dp"
                            android:layout_height="100dp"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:text="" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="50dp"
                    android:layout_marginTop="1dp" >

                    <TextView
                        android:id="@+id/textView16"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="10dp"
                        android:text="CD&apos;?"
                        android:textColor="#00478f"
                        android:textSize="23dp" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" >

                        <TextView
                            android:id="@+id/txt_cd2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@+id/textView17"
                            android:layout_alignParentTop="true"
                            android:layout_margin="1dp"
                            android:layout_toLeftOf="@+id/textView17"
                            android:background="#f1f1f1"
                            android:fadeScrollbars="false"
                            android:gravity="top"
                            android:hint="Assumption why is D&apos; necessary in order to get C? Complete the sentence &quot;In order to have D&apos;, I need to do C because...&quot;"
                            android:padding="5dp"
                            android:scrollbars="vertical"
                            android:text=""
                            android:textColor="#000000"
                            android:textSize="23dp" />

                        <EditText
                            android:id="@+id/edt_cd2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@+id/textView17"
                            android:layout_alignParentTop="true"
                            android:layout_toRightOf="@+id/textView17"
                            android:ems="10"
                            android:gravity="top"
                            android:hint="Create possible solutions (based on the assumptions and on intuition)"
                            android:inputType="textMultiLine"
                            android:padding="5dp"
                            android:textColor="#000000"
                            android:textSize="23dp" />

                        <TextView
                            android:id="@+id/textView17"
                            android:layout_width="10dp"
                            android:layout_height="100dp"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:text="" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="50dp"
                    android:layout_marginTop="2dp" >

                    <TextView
                        android:id="@+id/textView19"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="10dp"
                        android:text="DD&apos;?"
                        android:textColor="#00478f"
                        android:textSize="23dp" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" >

                        <TextView
                            android:id="@+id/txt_d1d2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@+id/textView20"
                            android:layout_alignParentTop="true"
                            android:layout_marginLeft="1dp"
                            android:layout_toLeftOf="@+id/textView20"
                            android:background="#f1f1f1"
                            android:fadeScrollbars="false"
                            android:gravity="top"
                            android:hint="Assumptions why are D and D&apos; in conflict?"
                            android:padding="5dp"
                            android:scrollbars="vertical"
                            android:text=""
                            android:textColor="#000000"
                            android:textSize="23dp" />

                        <EditText
                            android:id="@+id/edt_d1d2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@+id/textView20"
                            android:layout_alignParentTop="true"
                            android:layout_toRightOf="@+id/textView20"
                            android:ems="10"
                            android:gravity="top"
                            android:hint="Create possible solutions (based on the assumptions and on intuition)"
                            android:inputType="textMultiLine"
                            android:padding="5dp"
                            android:textColor="#000000"
                            android:textSize="23dp" />

                        <TextView
                            android:id="@+id/textView20"
                            android:layout_width="10dp"
                            android:layout_height="100dp"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:text="" />
                    </RelativeLayout>
                </LinearLayout>

                <TextView
                    android:id="@+id/error"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="10dp"
                    android:text="File name already exists"
                    android:textColor="#FF00FF"
                    android:visibility="gone" />

                <RelativeLayout
                    android:id="@+id/seek"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="10dp"
                    android:weightSum="10" >

                    <SeekBar
                        android:id="@+id/sBcompleted"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="5dp"
                        android:layout_weight="1"
                        android:max="100"
                        android:progress="0"
                        android:progressDrawable="@drawable/cuseekbar" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/seekcompleted"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="10dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:weightSum="10" >

                    <TextView
                        android:id="@+id/txtcompleted"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:paddingLeft="5dp"
                        android:text="completed"
                        android:textColor="#000000"
                        android:textSize="18dp" />

                    <TextView
                        android:id="@+id/tvcompleted"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_toLeftOf="@+id/txtcompleted"
                        android:layout_weight="2"
                        android:text="0%"
                        android:textColor="#000000"
                        android:textSize="18dp" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_below="@+id/rl2"
                    android:layout_marginTop="10dp"
                    android:orientation="vertical" >

                    <RelativeLayout
                        android:id="@+id/ff"
                        android:layout_width="match_parent"
                        android:layout_height="180dp"
                        android:layout_alignParentBottom="true" >

                        <View
                            android:id="@+id/view1"
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginRight="10dp"
                            android:background="#000000" />

                        <View
                            android:layout_width="1dp"
                            android:layout_height="match_parent"
                            android:layout_alignBottom="@+id/view4"
                            android:layout_alignLeft="@+id/view1"
                            android:background="#000000" />

                        <View
                            android:layout_width="1dp"
                            android:layout_height="match_parent"
                            android:layout_alignBottom="@+id/view4"
                            android:layout_toRightOf="@+id/gg2"
                            android:background="#000000" />

                        <RelativeLayout
                            android:id="@+id/gg1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:layout_marginRight="10dp" >
                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/gg2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignRight="@+id/gg1"
                            android:layout_below="@+id/gg1"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="10dp" >

                            <Button
                                android:id="@+id/btn_back"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentLeft="true"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="5dp"
                                android:layout_marginRight="10dp"
                                android:background="@drawable/back" />

                            <RelativeLayout
                                android:id="@+id/gg"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerHorizontal="true"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="10dp"
                                android:layout_marginTop="10dp" >

                                <Button
                                    android:id="@+id/btnstory"
                                    style="?android:attr/buttonStyleSmall"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentTop="true"

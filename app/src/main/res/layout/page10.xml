                            android:layout_height="wrap_content"
                            android:layout_toLeftOf="@+id/imghint"
                            android:layout_weight="1"
                            android:background="#f1f1f1"
                            android:ems="10"
                            android:gravity="top"
                            android:layout_marginRight="10dp"

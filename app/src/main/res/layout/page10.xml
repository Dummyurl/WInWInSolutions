                                    android:padding="5dp"
                                    android:text=""
                                    android:textColor="#000000"
                                    android:textSize="16dp" />
                            </RelativeLayout>

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" >

                                <TextView
                                    android:id="@+id/textView12"
                                    android:layout_width="35dp"
                                    android:layout_height="100dp"
                                    android:layout_marginTop="10dp"
                                    android:text="BD?"
                                    android:textColor="#00478f"
                                    android:textSize="16dp" />

                                <TextView
                                    android:id="@+id/txt_bd1"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_alignBottom="@+id/textView12"
                                    android:layout_alignParentTop="true"
                                    android:layout_marginLeft="1dp"
                                    android:layout_toRightOf="@+id/textView12"
                                    android:background="#f1f1f1"
                                    android:gravity="top"
                                    android:hint="Create possible solutions (based on the assumptions and on intuition)"
                                    android:padding="5dp"
                                    android:text=""
                                    android:textColor="#000000"
                                    android:textSize="16dp" />
                            </RelativeLayout>

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" >

                                <TextView
                                    android:id="@+id/textView16"
                                    android:layout_width="35dp"
                                    android:layout_height="100dp"
                                    android:layout_marginTop="10dp"
                                    android:text="CD&apos;?"
                                    android:textColor="#00478f"
                                    android:textSize="16dp" />

                                <TextView
                                    android:id="@+id/txt_cd2"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_alignBottom="@+id/textView16"
                                    android:layout_alignParentTop="true"
                                    android:layout_margin="1dp"
                                    android:layout_toRightOf="@+id/textView16"
                                    android:background="#f1f1f1"
                                    android:gravity="top"
                                    android:hint="Create possible solutions (based on the assumptions and on intuition)"
                                    android:padding="5dp"
                                    android:text=""
                                    android:textColor="#000000"
                                    android:textSize="16dp" />
                            </RelativeLayout>

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" >

                                <TextView
                                    android:id="@+id/textView19"
                                    android:layout_width="35dp"
                                    android:layout_height="100dp"
                                    android:layout_marginTop="10dp"
                                    android:text="DD&apos;?"
                                    android:textColor="#00478f"
                                    android:textSize="16dp" />

                                <TextView
                                    android:id="@+id/txt_d1d2"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_alignBottom="@+id/textView19"
                                    android:layout_alignParentTop="true"
                                    android:layout_margin="1dp"
                                    android:layout_toRightOf="@+id/textView19"
                                    android:background="#f1f1f1"
                                    android:gravity="top"
                                    android:hint="Create possible solutions (based on the assumptions and on intuition)"
                                    android:padding="5dp"
                                    android:text=""
                                    android:textColor="#000000"
                                    android:textSize="16dp" />
                            </RelativeLayout>
                        </LinearLayout>

                        <EditText
                            android:id="@+id/injections"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@+id/rrl1"
                            android:layout_alignParentTop="true"
                            android:layout_margin="1dp"
                            android:layout_toRightOf="@+id/textView4"
                            android:fadeScrollbars="false"
                            android:gravity="top"
                            android:hint="Create the solution and write it down\n\n• Check to see that the solution meets the needs of B and C: &quot;If I carry out/ensure the solution activity then I will attain B and C, because...&quot;.\n• Include reasons why the solution leads to satisfying the needs of B and C.\n• If the solution and the two reasons are inconsistent, work on the wording or select another option from the list of possible solutions on the left."
                            android:inputType="textMultiLine"
                            android:padding="5dp"
                            android:scrollbars="vertical"
                            android:textColor="#000000"
                            android:textSize="16dp" />

                        <TextView
                            android:id="@+id/textView4"
                            android:layout_width="10dp"
                            android:layout_height="650dp"
                            android:layout_centerHorizontal="true"
                            android:layout_marginRight="10dp"
                            android:text="" />

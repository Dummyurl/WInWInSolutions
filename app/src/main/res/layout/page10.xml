                        android:textColor="#00478f"
                        android:textSize="18dp" />

                    <TextView
                        android:id="@+id/textView23"
                        android:layout_width="60dp"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:text="" />

                    <TextView
